@model IEnumerable<Arbitration.Models.Notice>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ConsumerClaimant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.From)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Content)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NotificationOfFiling)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoticeOfArbitratorSelection)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArbitratorsDisclosures)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SignedOathDocument)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppointmentOfArbitrator)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Schedule)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SchedulingOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompletionOfHearing)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ConsumerClaimant.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.From)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Content)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NotificationOfFiling)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoticeOfArbitratorSelection)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ArbitratorsDisclosures)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SignedOathDocument)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AppointmentOfArbitrator)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Schedule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SchedulingOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompletionOfHearing)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
